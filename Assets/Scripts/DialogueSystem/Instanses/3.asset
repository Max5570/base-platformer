  sentences:
  - name: Boy
    sentence: "\u0414\u0435\u0434\u0443\u0448\u043A\u0430. \u0410 \u0432\u044B \u0441
      \u0442\u0435\u043C \u043F\u0440\u0438\u0437\u0440\u0430\u043A\u043E\u043C \u043F\u043E\u0434\u0440\u0443\u0436\u0438\u043B\u0438\u0441\u044C?"
  - name: Ded
    sentence: "\u0410\u0433\u0430, \u0435\u0449\u0435 \u0447\u0435\u0433\u043E! \u042F
      \u0435\u0449\u0435 \u0441 \u043F\u0440\u0438\u0437\u0440\u0430\u043A\u0430\u043C\u0438
      \u043D\u0435 \u0434\u0440\u0443\u0436\u0438\u043B."
  - name: Ded
    sentence: "\u041C\u043E\u0436\u0435\u0442 \u0432\u044B \u0441\u0442\u0430\u043D\u0435\u0442\u0435
      \u0434\u0440\u0443\u0437\u044C\u044F\u043C\u0438 \u0438 \u0442\u043E\u0433\u0434\u0430,
      \u043E\u043D \u0431\u0443\u0434\u0435\u0442 \u0442\u0435\u0431\u0435 \u043F\u043E\u043C\u043E\u0433\u0430\u0442\u044C?
      \u0410?"
  - name: Boy
    sentence: "\u041D\u0435\u0442! \u042F \u043D\u0435 \u0431\u0443\u0434\u0443 \u0434\u0440\u0443\u0436\u0438\u0442\u044C
      \u0441 \u043F\u0440\u0438\u0437\u0440\u0430\u043A\u043E\u043C!"
  - name: Ded
    sentence: "\u041D\u0443... \u041D\u0443... \u041F\u043E\u0436\u0430\u043B\u0443\u0439\u0441\u0442\u0430,
      \u0440\u0430\u0434\u0438 \u043C\u0435\u043D\u044F..."
  - name: Ded
    sentence: "\u041B\u0430\u0434\u043D\u043E. \u041B\u0430\u0434\u043D\u043E. \u0425\u043E\u0440\u043E\u0448\u043E,
      \u043C\u044B \u0441\u0442\u0430\u043B\u0438 \u0434\u0440\u0443\u0437\u044C\u044F\u043C\u0438."
